{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "node": "node ./bin/www",
    "start": "nodemon ./bin/www",
    "app": "npm  --prefix ../app run dev",
    "dev": "concurrently \"npm run start\" \"npm run app\"",
    "app:start": "npm --prefix ../app run start",
    "dev:prod": "concurrently \"npm run start\" \"npm run node\"",
    "install:app": "npm  --prefix ../app install",
    "install:api": "npm install",
    "install:all": "npm run app:start && npm run install:api",
    "app:build": "npm --prefix ../app run build",
    "start:prod": "npm run install:all && npm run node && npm run app"
  },
  "dependencies": {
    "app": "file:../app",
    "concurrently": "^8.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^3.0.1"
  }
}
